<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wzx.pro.dao.BigMoneyMapper">

    <resultMap id="BaseResultMap" type="com.wzx.pro.domain.money.BigMoney">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="time" property="time" jdbcType="VARCHAR"/>
        <result column="term" property="term" jdbcType="VARCHAR"/>
        <result column="one" property="one" jdbcType="INTEGER"/>
        <result column="two" property="two" jdbcType="VARCHAR"/>
        <result column="three" property="three" jdbcType="VARCHAR"/>
        <result column="four" property="four" jdbcType="VARCHAR"/>
        <result column="five" property="five" jdbcType="VARCHAR"/>
        <result column="big_one" property="bigOne" jdbcType="VARCHAR"/>
        <result column="big_two" property="bigTwo" jdbcType="VARCHAR"/>
        <result column="str" property="str" jdbcType="VARCHAR"/>
        <result column="last" property="last" jdbcType="VARCHAR"/>
    </resultMap>
    
    <select id="findBigMoney" parameterType="com.wzx.pro.domain.money.BigMoney"
            resultMap="BaseResultMap" >
      select * from big_money where one =#{one}
       and two = #{two}
       and three= #{three}
       and four= #{four}
       and five= #{five}
       and big_one = #{bigOne}
       and big_two= #{bigTwo}
    </select>

</mapper>


